{
  "name": "alpine-pitbull",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install .husky",
    "prettier": "prettier --write .",
    "lint": "next lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "pnpm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/zoom": "^3.12.0",
    "cookies-next": "^6.1.0",
    "gsap": "^3.13.0",
    "lenis": "^1.3.4",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.1.0",
    "rehype-raw": "^7.0.0",
    "sass": "^1.87.0",
    "split-type": "^0.3.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "typescript": "^5"
  }
}
